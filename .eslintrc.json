{
    "env": {
        "browser": true,
        "es6": true      
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "require-jsdoc": "off",
        "no-console": "off",
        "max-len": [2, {"code": 200, "tabWidth": 4, "ignoreUrls": true}]
    }
}
